{
  "provider": "aws-s3-sharp-resize",
  "providerOptions": {
    "accessKeyId": "${process.env.ACCESS_KEY_ID}",
    "secretAccessKey": "${process.env.SECRET_ACCESS_KEY}",
    "endpoint": "ams3.digitaloceanspaces.com",
    "optimize": {
      "resize": false
    },
    "settings": {
      "awsUploadFolder": "media"
    },
    "params": {
      "Bucket": "berendkalberg.nl-uploads"
    }
  }
}
